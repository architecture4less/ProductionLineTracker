<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<TabPane prefHeight="450.0" prefWidth="350.0" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/1.8.0"
         xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.jwotoole9141.prodsline.Controller">
    <Tab text="Production Line">
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
            <VBox layoutX="607.0" layoutY="29.0" prefHeight="180.0" prefWidth="800.0" AnchorPane.bottomAnchor="12.0"
                  AnchorPane.leftAnchor="12.0" AnchorPane.rightAnchor="12.0" AnchorPane.topAnchor="12.0">
                <GridPane minHeight="90.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <VBox.margin>
                        <Insets bottom="4.0"/>
                    </VBox.margin>
                    <Label text="Product Name"/>
                    <Label text="Manufacturer" GridPane.rowIndex="1"/>
                    <Label text="Item Type" GridPane.rowIndex="2"/>
                    <TextField fx:id="fldNewProdName" GridPane.columnIndex="1"/>
                    <TextField fx:id="fldNewProdManuf" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                    <ChoiceBox fx:id="chbNewProdType" prefWidth="9.999999999E9" GridPane.columnIndex="1"
                               GridPane.rowIndex="2"/>
                </GridPane>
                <HBox>
                    <Pane HBox.hgrow="ALWAYS">
                        <HBox.margin>
                            <Insets/>
                        </HBox.margin>
                        <padding>
                            <Insets bottom="5.0"/>
                        </padding>
                    </Pane>
                    <Button fx:id="btnAddProd" mnemonicParsing="false" onAction="#addProduct" text="Add Product"/>
                </HBox>
                <Label text="Existing Products">
                    <VBox.margin>
                        <Insets bottom="4.0" top="12.0"/>
                    </VBox.margin>
                </Label>
                <TableView fx:id="tblProducts">
                    <columns>
                        <TableColumn prefWidth="75.0" text="C1"/>
                        <TableColumn prefWidth="75.0" text="C2"/>
                    </columns>
                </TableView>
            </VBox>
        </AnchorPane>
    </Tab>
    <Tab text="Produce">
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
            <VBox layoutX="459.0" layoutY="129.0" prefHeight="571.0" prefWidth="800.0" AnchorPane.bottomAnchor="12.0"
                  AnchorPane.leftAnchor="12.0" AnchorPane.rightAnchor="12.0" AnchorPane.topAnchor="12.0">
                <Label text="Choose Product">
                    <VBox.margin>
                        <Insets bottom="4.0"/>
                    </VBox.margin>
                </Label>
                <ListView fx:id="lstProdsOpts" prefHeight="200.0" prefWidth="402.0" VBox.vgrow="ALWAYS"/>
                <Label text="Choose Quantity">
                    <VBox.margin>
                        <Insets bottom="4.0" top="12.0"/>
                    </VBox.margin>
                </Label>
                <ComboBox fx:id="cboProdsQnty" prefWidth="150.0">
                    <VBox.margin>
                        <Insets bottom="12.0"/>
                    </VBox.margin>
                </ComboBox>
            </VBox>
        </AnchorPane>
    </Tab>
    <Tab text="Production Log">
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
            <TextArea fx:id="txtProdsLog" layoutX="14.0" layoutY="14.0" prefHeight="571.0" prefWidth="800.0"
                      AnchorPane.bottomAnchor="12.0" AnchorPane.leftAnchor="12.0" AnchorPane.rightAnchor="12.0"
                      AnchorPane.topAnchor="12.0"/>
        </AnchorPane>
    </Tab>
</TabPane>
